{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "js": [ "jquery-3.2.1.min.js", "espn.js" ],
      "matches": [ "*://www.espn.com/*" ]
   } ],
   "description": "Remove mentions of the Ball family from ESPN and other sports websites so that you can focus on actual basketball news.",
   "icons": {
      "128": "icon-128.png",
      "16": "icon-16.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Big Baller Blocker",
   "permissions": [ "*://www.espn.com/*", "*://www.sbnation.com/*", "*://www.foxsports.com/*", "*://www.theringer.com/*", "tabs", "webNavigation" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.3"
}
